<h1>Activo</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nrobiennacional</th>
            <td>{{ entity.nroBienNacional }}</td>
        </tr>
        <tr>
            <th>Observacion</th>
            <td>{{ entity.observacion }}</td>
        </tr>
        <tr>
            <th>Creado</th>
            <td>{{ entity.creado|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Actualizado</th>
            <td>{{ entity.actualizado|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('activo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('activo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('activo_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
